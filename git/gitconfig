[user]
	email = yuqiao@swoo.ca
	name = Yu Qiao Chen
[core]
	excludesfile = /Users/swoo/.swoo-workspace/.gitignore
	editor = nvim
	hooksPath = /Users/swoo/.swoo-workspace/hooks
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[alias]
	clean-branches = !git branch | egrep -v \"(main|\\*)\" | xargs git branch -D
[init]
    defaultBranch = main
[pull]
    rebase = false
[gpg]
    program = gpg
